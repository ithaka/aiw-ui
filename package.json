{
  "name": "avatar",
  "version": "1.12.29",
  "description": "Front End for the Artstor Digital Library",
  "author": "Artstor Team Air",
  "license": "MIT",
  "scripts": {
    "build:dev": "ng build --aot",
    "build:prod": "ng build --prod",
    "build": "ng build",
    "bump": "npm run bump:patch",
    "bump:patch": "yarn version --new-version patch",
    "bump:minor": "yarn version --new-version minor",
    "bump:major": "yarn version --new-version major",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist",
    "dev": "ng serve",
    "dev:https": "ng serve --ssl true",
    "dev:ssl": "ng serve --ssl true",
    "sahara": "ng serve --host local.sahara.test.artstor.org",
    "deploy:test": "npm run bump:patch && git push test stage:master",
    "deploy:test:patch": "npm run bump:patch && git push test stage:master",
    "deploy:test:minor": "npm run bump:minor && git push test stage:master",
    "deploy:test:major": "npm run bump:major && git push test stage:master",
    "deploy:prod": "git push prod master",
    "deploy:ssr:test": "npm run docker:build && ./deploy-ssr.sh test",
    "deploy:ssr:prod": "npm run docker:build && ./deploy-ssr.sh prod",
    "docker:build": "./docker-build-image.sh",
    "docker": "docker",
    "docs": "npm run typedoc && npm run styleguide",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "local": "ng serve --host local.artstor.org",
    "pact": "ng test --main src/pact.ts",
    "pact:publish": "rm -rf pacts && yarn pact && node config/pact.publish.js",
    "postversion": "git push && git push --tags",
    "postinstall": "node ./ng-add-pug-loader.js",
    "protractor": "protractor",
    "start": "ng serve",
    "styleguide": "./node_modules/.bin/node-sass src/sass/app.scss src/assets/styleguide/styles/all.css && nucleus --config nucleus/config.nucleus.json",
    "open:styleguide": "open ./src/assets/styleguide/index.html",
    "test": "ng test",
    "tslint": "tslint",
    "typedoc": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/ && npm run open:typedoc",
    "open:typedoc": "open ./doc/index.html",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:ssr:dev": "ng build --prod --build-optimizer=false && ng run avatar:server --optimization=false && npm run webpack:server",
    "build:client-and-server-bundles": "ng build --prod && ng run avatar:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
