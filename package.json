{
  "name": "avatar",
  "version": "2.10.42",
  "description": "Front End for the Artstor Digital Library",
  "author": "Artstor Team Air",
  "license": "MIT",
  "scripts": {
    "build:dev": "ng build --aot",
    "build:prod": "ng build --prod",
    "build": "ng build",
    "bump": "npm run bump:patch",
    "bump:patch": "yarn version --new-version patch",
    "bump:minor": "yarn version --new-version minor",
    "bump:major": "yarn version --new-version major",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist",
    "dev": "ng serve",
    "dev:ssl": "ng serve --ssl --ssl-key ssl/private.key --ssl-cert ssl/private.crt",
    "dev:https": "npm run dev:ssl",
    "dev:ssl:cli": "ng serve --ssl true",
    "dev:ssr": "npm run build:client-and-server-bundles && npm run webpack:server && npm run serve:ssr",
    "dev:ssr:server": "npm run webpack:server && npm run serve:ssr",
    "sahara": "ng serve --host local.sahara.test.artstor.org",
    "deploy:test": "./docker-build-image.sh test && ./deploy-ssr.sh test",
    "deploy:prod": "./docker-build-image.sh prod && ./deploy-ssr.sh prod",
    "docker:build": "./docker-build-image.sh",
    "docker": "docker",
    "docs": "npm run typedoc && npm run styleguide",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "local": "ng serve --host local.artstor.org",
    "pact": "ng test --main src/pact.ts",
    "pact:publish": "rm -rf pacts && yarn pact && node config/pact.publish.js",
    "postversion": "git push && git push --tags",
    "postinstall": "node ./ng-add-pug-loader.js",
    "protractor": "protractor",
    "start": "ng serve",
    "styleguide": "./node_modules/.bin/node-sass src/sass/app.scss src/assets/styleguide/styles/all.css && nucleus --config nucleus/config.nucleus.json",
    "open:styleguide": "open ./src/assets/styleguide/index.html",
    "test": "ng test",
    "tslint": "tslint",
    "typedoc": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/ && npm run open:typedoc",
    "open:typedoc": "open ./doc/index.html",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:ssr:test": "npm run build:client-and-server-bundles:test && npm run webpack:server -- --env=test",
    "build:ssr:prod": "npm run build:client-and-server-bundles:prod && npm run webpack:server -- --env=prod",
    "serve:ssr": "node dist/server.js",
    "serve:ssr:start": "pm2 start dist/server.js",
    "serve:ssr:stop": "pm2 stop dist/server.js",
    "build:client-and-server-bundles": "ng build --configuration=stage && ng run avatar:server",
    "build:client-and-server-bundles:test": "ng build --configuration=stage && ng run avatar:server",
    "build:client-and-server-bundles:prod": "ng build --configuration=production && ng run avatar:server",
    "webpack:server": "webpack --config ./webpack.server.config.js --progress --colors",
    "webpack:server:test": "npm run webpack:server -- --env=test",
    "webpack:server:prod": "npm run webpack:server -- --env=prod"
  },
