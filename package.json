        "karma-phantomjs-launcher": "^1.0.2",
        "karma-remap-coverage": "^0.1.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.13",
        "parse5": "^1.5.1",
        "phantomjs": "^2.1.7",
        "protractor": "^5.1.0",
        "protractor-cucumber-framework": "^0.6.0",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.4",
        "sass-loader": "^6.0.7",
        "source-map-loader": "^0.2.3",
        "string-replace-loader": "^2.1.1",
        "style-loader": "^0.20.2",
        "to-string-loader": "^1.1.5",
        "ts-helpers": "^1.1.1",
        "ts-node": "^1.7.0",
        "tslint": "^4.4.2",
        "tslint-loader": "^3.6.0",
        "typedoc": "^0.5.1",
        "typescript": "2.7.2",
        "webpack": "4.1.0",
        "webpack-cli": "^2.0.10",
        "webpack-closure-compiler": "^2.1.6",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-dev-server": "^3.1.4",
        "webpack-md5-hash": "0.0.6",
        "webpack-merge": "^4.1.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ithaka/aiw-ui.git"
    },
    "bugs": {
        "url": "https://jira.jstor.org/projects/AIR/summary"
    },
    "engines": {
        "node": ">= 8.11.1",
        "npm": ">= 4"
    }
}
