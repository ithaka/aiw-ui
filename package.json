{
  "name": "avatar",
  "version": "0.0.5",
  "description": "Front End for the Artstor Digital Library",
  "author": "Artstor Team Air",
  "license": "",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --profile --bail",
    "build": "npm run build:dev",
    "bump": "npm run bump:patch",
    "bump:patch": "yarn version --new-version patch",
    "bump:minor": "yarn version --new-version minor",
    "bump:major": "yarn version --new-version major",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docker": "docker",
    "docs": "npm run typedoc && npm run styleguide",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --profile --github-prod",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "styleguide": "./node_modules/.bin/node-sass src/sass/app.scss styleguide/styles/all.css && nucleus --config styleguide/config.nucleus.json && npm run open:styleguide",
    "open:styleguide": "open ./styleguide/index.html",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/ && npm run open:typedoc",
    "open:typedoc": "open ./doc/index.html",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/platform-server": "2.4.1",
    "@angular/router": "3.4.1",
    "@angularclass/conventions-loader": "^1.0.13",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.14",
    "@ng-idle/core": "^2.0.0-beta.8",
    "@ng-idle/keepalive": "^2.0.0-beta.8",
    "angular2-locker": "^0.5.0",
    "angular2-medium-editor": "^1.0.5",
    "angular2-moment": "^1.2.0",
    "angular2-tag-input": "^1.2.3",
    "angulartics2": "^1.6.3",
    "assets-webpack-plugin": "^3.5.0",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "bootstrap": "4.0.0-alpha.6",
    "codelyzer": "^2.0.0-beta.1",
    "core-js": "^2.4.1",
    "cucumber": "^2.0.0-rc.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "medium-editor": "^5.23.0",
    "ng2-dnd": "^4.0.0",
    "ng2-translate": "^5.0.0",
    "openseadragon": "^2.2.1",
    "rxjs": "^5.0.1",
    "url-loader": "^0.5.7",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.49",
    "@types/protractor": "^1.5.20",
    "@types/selenium-webdriver": "2.44.29",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.36",
    "angular2-template-loader": "^0.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^1.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.13.0",
    "parse5": "^1.5.1",
    "phantomjs": "^2.1.7",
    "protractor": "^5.1.0",
    "protractor-cucumber-framework": "^0.6.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.1",
    "ts-node": "^1.7.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.10",
    "webpack": "2.1.0-beta.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "2.1.0-beta.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angularclass/angular2-webpack-starter.git"
  },
  "bugs": {
