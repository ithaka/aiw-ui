{
    "name": "avatar",
    "version": "1.8.12",
    "description": "Front End for the Artstor Digital Library",
    "author": "Artstor Team Air",
    "license": "",
    "scripts": {
        "build:dev": "webpack --config config/webpack.dev.js --profile",
        "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
        "build:prod": "webpack --config config/webpack.prod.js",
        "build": "npm run build:dev",
        "bump": "npm run bump:patch",
        "bump:patch": "yarn version --new-version patch",
        "bump:minor": "yarn version --new-version minor",
        "bump:major": "yarn version --new-version major",
        "ci": "npm run lint && npm test && npm run e2e",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "clean:start": "npm start",
        "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist",
        "dev": "npm run server:dev",
        "sahara": "yarn run server:sahara",
        "deploy:test": "npm run bump:patch && git push test master",
        "deploy:test:patch": "npm run bump:patch && git push test master",
        "deploy:test:minor": "npm run bump:minor && git push test master",
        "deploy:test:major": "npm run bump:major && git push test master",
        "deploy:prod": "git push prod master",
        "docker": "docker",
        "docs": "npm run typedoc && npm run styleguide",
        "e2e:live": "npm run e2e -- --elementExplorer",
        "e2e": "npm run protractor",
        "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --profile --github-dev",
        "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --profile --github-prod",
        "github-deploy": "npm run github-deploy:dev",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "local": "yarn run server:local",
        "postversion": "git push && git push --tags",
        "prebuild:dev": "npm run clean:dist",
        "prebuild:prod": "npm run clean:dist",
        "preclean:install": "npm run clean",
        "preclean:start": "npm run clean",
        "pree2e": "npm run webdriver:update -- --standalone",
        "protractor": "protractor",
        "rimraf": "rimraf",
        "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
        "server:sahara": "webpack-dev-server --config config/webpack.dev.js --profile --watch --content-base src/ --host 'local.sahara.test.artstor.org'",
        "server:local": "webpack-dev-server --config config/webpack.dev.js --profile --watch --content-base src/ --host 'local.artstor.org'",
        "server:prod": "http-server dist --cors",
        "server": "npm run server:dev",
        "start": "npm run server:dev",
        "styleguide": "./node_modules/.bin/node-sass src/sass/app.scss styleguide/styles/all.css && nucleus --config styleguide/config.nucleus.json && npm run open:styleguide",
        "open:styleguide": "open ./styleguide/index.html",
        "test": "karma start",
        "tslint": "tslint",
        "typedoc": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/ && npm run open:typedoc",
        "open:typedoc": "open ./doc/index.html",
        "watch:dev": "npm run build:dev -- --watch",
        "watch:prod": "npm run build:prod -- --watch",
        "watch:test": "npm run test -- --auto-watch --no-single-run",
        "watch": "npm run watch:dev",
        "webdriver-manager": "webdriver-manager",
        "webdriver:start": "npm run webdriver-manager start",
        "webdriver:update": "npm run webdriver-manager update",
        "webpack-dev-server": "webpack-dev-server",
        "webpack": "webpack"
    },
    "dependencies": {
        "@angular/animations": "^5.2.1",
        "@angular/common": "^5.2.1",
        "@angular/compiler": "^5.2.1",
        "@angular/core": "^5.2.1",
        "@angular/forms": "^5.2.1",
        "@angular/http": "^5.2.1",
        "@angular/platform-browser": "^5.2.1",
        "@angular/platform-browser-dynamic": "^5.2.1",
        "@angular/platform-server": "^5.2.1",
        "@angular/router": "^5.2.1",
        "@angularclass/conventions-loader": "^1.0.13",
        "@angularclass/hmr": "^1.2.2",
        "@angularclass/hmr-loader": "^3.0.2",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
        "@ng-idle/core": "^2.0.0-beta.13",
        "@ng-idle/keepalive": "^2.0.0-beta.13",
        "@ngx-translate/core": "^9.0.1",
        "@ngx-translate/http-loader": "^2.0.1",
        "angular2-click-outside": "^0.1.0",
        "angular2-locker": "^0.5.0",
        "angular2-medium-editor": "1.1.0",
        "angular2-moment": "^1.2.0",
        "angular2-tag-autocomplete": "^1.2.10",
        "angulartics2": "^4.2.0",
        "artstor-viewer": "1.1.3",
        "assets-webpack-plugin": "^3.5.1",
        "blob-polyfill": "^2.0.20171115",
        "codelyzer": "^2.0.0-beta.1",
        "core-js": "^2.4.1",
        "ie-shim": "^0.1.0",
        "linkifyjs": "^2.1.4",
        "medium-editor": "^5.23.0",
        "ng2-completer": "^1.4.0",
        "ng2-device-detector": "^1.0.0",
        "ng2-dnd": "^4.0.0",
        "ng2-file-upload": "^1.3.0",
        "node-sass": "^4.9.0",
        "openseadragon": "^2.2.1",
        "pug": "^2.0.0-rc.4",
        "pug-html-loader": "^1.1.5",
        "raven-js": "^3.20.1",
        "rxjs": "^5.5.2",
        "url-loader": "^1.0.1",
        "zone.js": "^0.6.26"
    },
    "devDependencies": {
        "@types/hammerjs": "^2.0.33",
        "@types/jasmine": "^2.5.38",
        "@types/node": "^6.0.49",
        "@types/protractor": "^1.5.20",
        "@types/selenium-webdriver": "2.44.29",
        "@types/source-map": "^0.1.27",
        "@types/uglify-js": "^2.0.27",
        "@types/webpack": "^1.12.36",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "5.0.0-0",
        "bootstrap": "4.0.0-beta.2",
        "compression-webpack-plugin": "^1.1.10",
        "copy-webpack-plugin": "^4.5.0",
        "css-loader": "^0.28.10",
        "cucumber": "^2.0.0-rc.2",
        "exports-loader": "^0.7.0",
        "expose-loader": "^0.7.4",
        "file-loader": "^1.1.11",
        "gh-pages": "^0.11.0",
        "grunt": "^1.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.0.4",
        "http-server": "^0.9.0",
        "imports-loader": "^0.8.0",
        "istanbul-instrumenter-loader": "^3.0.0",
        "json-loader": "^0.5.7",
        "karma": "^1.4.1",
